   ],
   "source": [
    "#slicing in 1D array\n",
    "x=np.array([[4,6,3],[7,5,8],[9,4,8]])\n",
    "print(x[1,1])\n",
    "print(x[1:3,1:3])\n",
    "print(x[1,0:2])"
   ]
