    }
   ],
   "source": [
    "#slicing in 1D array\n",
    "import numpy as np\n",
    "arr=np.array([3,4,7,53,5,6,6])\n",
    "print(arr[1])\n",
    "print(arr[1:2])\n",
    "print(arr[:3])\n",
    "print(arr[1:])\n"
   ]
  },
  {
   "cell_type": "code",
